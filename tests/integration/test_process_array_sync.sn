// Test sync list synchronization with Process variables
// Cross-platform using Python

fn main(): int =>
    var p1: Process = &Process.run("python", {"-c", "print('one')"})
    var p2: Process = &Process.run("python", {"-c", "print('two')"})
    var p3: Process = &Process.run("python", {"-c", "print('three')"})

    print("spawned all\n")

    [p1, p2, p3]!

    print("synced all\n")

    print($"p1: {p1.stdout.trim()}\n")
    print($"p2: {p2.stdout.trim()}\n")
    print($"p3: {p3.stdout.trim()}\n")

    print($"exits: {p1.exitCode}, {p2.exitCode}, {p3.exitCode}\n")

    return 0
