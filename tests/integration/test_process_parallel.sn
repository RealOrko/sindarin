// Test multiple concurrent processes
// Cross-platform using Python

fn main(): int =>
    var p1: Process = &Process.run("python", {"-c", "print('first')"})
    var p2: Process = &Process.run("python", {"-c", "print('second')"})
    var p3: Process = &Process.run("python", {"-c", "print('third')"})

    print("all spawned\n")

    p1!
    p2!
    p3!

    print("all done\n")

    print($"p1: {p1.stdout.trim()}\n")
    print($"p2: {p2.stdout.trim()}\n")
    print($"p3: {p3.stdout.trim()}\n")

    var total: int = p1.exitCode + p2.exitCode + p3.exitCode
    print($"total exit: {total}\n")

    return 0
